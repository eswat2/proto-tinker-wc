{
  "name": "proto-tinker-wc",
  "version": "0.1.57",
  "description": "prototype - simple demo built from Stencil & Tailwind",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/proto-tinker-wc/proto-tinker-wc.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "dev": "concurrently \"yarn:twx --watch\" \"yarn:start\"",
    "twx": "sh .generate",
    "build": "yarn twx; yarn format; stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "echo --skip",
    "generate": "stencil generate",
    "predeploy": "yarn build",
    "deploy": "yarn publish --patch",
    "spell": "cspell 'src/**/*.{ts,tsx,html,css,md}' ",
    "cc": "prettier --check src",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@stencil/core": "4.35.1",
    "@stencil/store": "2.1.3",
    "between.js": "0.1.2-fix.2"
  },
  "devDependencies": {
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.0",
    "cspell": "9.1.2",
    "eslint": "9.29.0",
    "postcss": "8.5.6",
    "prettier": "3.6.1",
    "prettier-plugin-tailwindcss": "0.6.13",
    "proto-tailwindcss-clrs": "0.0.423",
    "tailwindcss": "4.1.10",
    "typescript": "5.8.3",
    "workbox-build": "7.3.0"
  },
  "license": "MIT",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
